*fontList:	-*-gothic-medium-r-normal-*-12-*-*-*-p-*-*-1

URxvt.print-pipe: "cat > /dev/null"
URxvt.foreground:         white
URxvt.background:         black
URxvt.font: xft:DjvsmHW+VL-11
URxvt.keysym.M-C-1: command:\033]710;xft:DjvsmHW+VL-15:pixelsize=18\007
URxvt.keysym.M-C-2: command:\033]710;xft:DjvsmHW+VL-13:pixelsize=16\007
URxvt.keysym.M-C-3: command:\033]710;xft:DjvsmHW+VL-11:pixelsize=14\007
URxvt.saveLines: 10240
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt.preeditType: OverTheSpot
URxvt.letterSpace: -1
URxvt.lineSpace: 0
URxvt.inputMethod: xim
URxvt*modifier: alt
URxvt.backspacekey: 
URxvt.cursorColor: red
URxvt.cursorBlink: true


Emacs*useXim: false

